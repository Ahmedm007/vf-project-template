    "@aws-sdk/util-dynamodb": "^3.113.0",
    "@shelf/jest-dynamodb": "^2.2.4",
    "@ttec-dig-vf/cdk-resources": "^7.13.0",
    "@ttec-dig-vf/vf-logger": "^0.1.6",
    "@types/aws-lambda": "^8.10.101",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.41",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
